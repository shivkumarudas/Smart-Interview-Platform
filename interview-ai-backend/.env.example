# MongoDB (required for auth/profile/feedback)
MONGO_URI=mongodb://127.0.0.1:27017/interviewai

# Groq (required for interview question/evaluation)
GROQ_API_KEY=your_groq_api_key_here
# Optional: Groq model override
# GROQ_MODEL=llama-3.1-8b-instant

# Server port
PORT=5000

# Optional: restrict CORS for separate-frontend development
# CORS_ORIGIN=http://127.0.0.1:5500

# Optional: require DB to boot (enabled automatically in production)
# REQUIRE_DB=false
